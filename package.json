    "*.+(ts|tsx|js|jsx)": "yarn eslint  --ext .js,.jsx,.ts,.tsx --fix src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
